        </header>

        <div class="tableRank">
            <div class="tableWrapper">
                <table border="1">
                    <thead>
                        <tr>
                            <th>ID_User</th>
                            <th>Tên</th>
                            <th>Màn</th>
                            <th>Điểm</th>
                            <th>Coin</th>
                            <th>Trạng Thái</th>
                            <th>Tools</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each AllUser}}
                        <tr>
                            <td>{{this._id}}</td>
                            <td>{{this.name}}</td>
                            <td>{{this.man}}</td>
                            <td>{{this.diem}}</td>
                            <td>{{this.coin}}</td>
                            <td>
                                <select>
                                    <option >Không ban</option>
                                    <option >Đã ban</option>
                                </select>
                            </td>
                            <td><button>Xóa</button>|<button>Sữa</button></td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
